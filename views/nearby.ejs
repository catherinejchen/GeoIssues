<!DOCTYPE html>

<html>

<head>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta charset="utf-8">
    <link href='https://fonts.googleapis.com/css?family=Great Vibes' rel='stylesheet'>
    <link href="StyleSheet.css" rel="StyleSheet" type="text/css">
    <title>Nearby</title>
</head>

<body>
    <div id="map"></div>
    </div>
    <script>

        var map;
        function initMap() {
            map = new google.maps.Map(document.getElementById('map'), {
                center: {
                    lat: 49.5,
                    lng: -123.121
                },
                zoom: 14,
                zoomControl: false,
                mapTypeControl: false,
                fullscreenControl: false,
                scaleControl: false,
                streetViewControl: false
            });
            if (navigator.geolocation) {
                navigator.geolocation.getCurrentPosition(function (position) {
                    lat = position.coords.latitude;
                    lng = position.coords.longitude;
                    map.setCenter(new google.maps.LatLng(lat, lng));
                    var Geocoder = new google.maps.Geocoder;

                    function geocodeLatLng(latlng, cb) {
                        return Geocoder.geocode({
                            'location': latlng
                        }, function (res, status) {
                            return cb((status === 'OK') ? (res[1] || "Nothing found") : status);
                        });
                    }
                });
            }
            var infowindow = new google.maps.InfoWindow();

            var marker, i;
            var x = <%-JSON.stringify(data)%>;
            console.log(x);
            Object.keys(x).forEach(function(key) {
                marker = new google.maps.Marker({
                position: new
                google.maps.LatLng(x[key].latitude, x[key].longitude),
                map: map
                });
                google.maps.event.addListener(marker, 'click',
                (function (marker, i) {
                return function () {
                let html = "<div class='info-window'><h1>" + x[key].title + "</h1><img width=150 src='" + x[key].image + "'>" + "<p>" + x[key].description + "</p>" + "<h3>" + x[key].time + "</h3>" + "<h3>" + x[key].date + "</h3>" + "<h3>Category: " + x[key].category + "</h3>" + "</div>";
                infowindow.setContent(html);
                infowindow.open(map, marker);
                }
                })(marker, i));
            });
        }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCGVFxlXZXgKNSabKaxNb82u9ex126Ib48&callback=initMap"
        async defer></script>
    <script src="Script.js"></script>
</body>

</html>